%div.content
  %h1 Speakers#edit

  %div.speaker_form
    = form_for @speaker, html: { multipart: true } do |f|
      = render :partial => "form", :locals => {:f => f, :speaker => @speaker}


  - if !@errors.nil?
    %ul.Signup_Errors
    - for message_error in @errors
      %li
        * #{message_error}